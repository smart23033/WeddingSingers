<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".function.mypage.pointstore.PointStoreActivity">

    <FrameLayout
        android:id="@+id/point_store_fl_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize">

        <android.support.v7.widget.Toolbar
            android:id="@+id/point_store_toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:title="" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="포인트 상점" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/act_point_store_fl_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/point_store_fl_toolbar" />

</RelativeLayout>
