<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content" android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/view_vertical_margin"
    android:layout_marginBottom="@dimen/view_vertical_margin"
    android:layout_marginLeft="@dimen/view_horizontal_margin"
    android:layout_marginRight="@dimen/view_horizontal_margin"
    >

    <ImageView
        android:layout_width="@dimen/view_search_result_iv_thumbnail_width"
        android:layout_height="@dimen/view_search_result_iv_thumbnail_height"
        android:id="@+id/view_search_result_iv_thumbnail"
        android:src="@mipmap/ic_launcher"
        android:layout_marginRight="@dimen/view_search_result_iv_thumbnail_margin_right"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view_search_result_tv_title"
        android:text="title"
        android:layout_toRightOf="@id/view_search_result_iv_thumbnail"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view_search_result_tv_divider"
        android:text="|"
        android:layout_toRightOf="@id/view_search_result_tv_title"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        android:layout_marginLeft="@dimen/view_search_result_tv_divider_horizontal_margin"
        android:layout_marginRight="@dimen/view_search_result_tv_divider_horizontal_margin"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view_search_result_tv_singer_name"
        android:text="singer name"
        android:layout_toRightOf="@id/view_search_result_tv_divider"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view_search_result_tv_date"
        android:text="2016. 05. 24"
        android:layout_alignStart="@+id/view_search_result_tv_title"
        android:layout_alignLeft="@id/view_search_result_tv_title"
        android:layout_below="@id/view_search_result_tv_title"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        />

    <ImageView
        android:layout_width="@dimen/view_search_result_iv_favorite_width"
        android:layout_height="@dimen/view_search_result_iv_favorite_height"
        android:id="@+id/view_search_result_iv_favorite"
        android:src="@drawable/search_ic_num_of_favorite"
        android:layout_alignStart="@id/view_search_result_tv_date"
        android:layout_alignLeft="@id/view_search_result_tv_date"
        android:layout_below="@id/view_search_result_tv_date"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view_search_result_tv_num_of_favorite"
        android:text="112"
        android:layout_toRightOf="@id/view_search_result_iv_favorite"
        android:layout_below="@id/view_search_result_tv_date"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        android:layout_marginLeft="@dimen/view_search_result_tv_num_of_favorite_margin_left"/>

    <ImageView
        android:layout_width="@dimen/view_search_result_iv_hit_width"
        android:layout_height="@dimen/view_search_result_iv_hit_height"
        android:id="@+id/view_search_result_iv_hit"
        android:src="@drawable/search_ic_hit"
        android:layout_marginLeft="@dimen/view_search_result_iv_hit_margin_left"
        android:layout_below="@id/view_search_result_tv_date"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        android:layout_toRightOf="@id/view_search_result_tv_num_of_favorite"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view_search_result_tv_hit"
        android:text="350"
        android:layout_toRightOf="@+id/view_search_result_iv_hit"
        android:layout_below="@id/view_search_result_tv_date"
        android:layout_marginTop="@dimen/view_search_result_tv_margin_top"
        android:layout_marginLeft="@dimen/view_search_result_tv_hit_margin_left"/>

</RelativeLayout>